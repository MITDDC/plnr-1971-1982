				     (THERT ODD NUMBER OF GOODIES - THSETQ))
		   ((ATOM (CAR THL))
		    (THPUSH THML (LIST (QUOTE SETQ)
				       (CAR THL)
				       (LIST (QUOTE QUOTE) (EVAL (CAR THL)))))
		    (SET (CAR THL) (SETQ THVALUE (EVAL (CADR THL)))))
		   (T (THRPLACAS (CDR (THSGAL (CAR THL)))
				 (SETQ THVALUE (THVAL (CADR THL) THALIST)))))
	     (SETQ THL (CDDR THL))
	     (GO LOOP)))

