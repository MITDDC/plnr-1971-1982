(DECLARE (SPECIAL THTREE))

(DEFUN THFIND
       FEXPR
       (THA)
       (THBIND (CADDR THA))
       (THPUSH THTREE
	       (LIST (QUOTE THFIND)
		     (COND ((EQ (CAR THA) (QUOTE ALL)) (QUOTE (1 NIL NIL)))
			   ((ATOM (CAR THA)) (LIST (CAR THA) (CAR THA) T))
			   ((CAR THA)))
		     (CONS 0 NIL)
		     (CADR THA)))
       (THPUSH THTREE (LIST (QUOTE THPROG) (CDDR THA) NIL (CDDR THA)))
       (THPROGA))

