(DEFUN THSGAL
       (X)
       (SASSQ (CADR X)
	      THALIST
	      (FUNCTION (LAMBDA NIL
				(PROG (Y)
				      (SETQ Y (LIST (CADR X)
						    (QUOTE THUNASSIGNED)))
				      (NCONC (GET (QUOTE THALIST) (QUOTE VALUE))
					     (LIST Y))
				      (RETURN Y))))))

